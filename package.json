{
  "name": "thrustcurve-db",
  "version": "1.0.0",
  "type": "module",
  "description": "ThrustCurve.org model rocket motor and thrust curve data as a single JSON file",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "types": "thrustcurve-db.d.ts",
  "files": [
    "index.js",
    "util.js",
    "thrustcurve-db.json",
    "thrustcurve-db.d.ts"
  ],
  "scripts": {
    "prepare": "node build/build.js > thrustcurve-db.json",
    "test": "node --experimental-json-modules build/test.js && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broofa/thrustcurve-db.git"
  },
  "keywords": [
    "rocketry",
    "rocket",
    "motor",
    "thrustcurve",
    "thrust",
    "json",
    "data",
    "database"
  ],
  "author": "Robert Kieffer <robert@broofa.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/broofa/thrustcurve-db/issues"
  },
  "homepage": "https://github.com/broofa/thrustcurve-db#readme",
  "devDependencies": {
    "ansi": "0.3.1",
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.10",
    "cli-spinners": "2.6.0"
  }
}
